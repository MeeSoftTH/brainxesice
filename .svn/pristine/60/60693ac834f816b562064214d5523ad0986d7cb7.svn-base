import Foundation

class GameScore {
    var score: Int = 0
    var level: Int = 1
    var incorrect: Int = 0
    
    required init(score: Int, incorrect: Int, level : Int){
        self.score = score
        self.incorrect = incorrect
        self.level = level
    }
}