import Foundation

class GameScore {
    var score: Int = 0
    var level: Int = 1
    var correct: Int = 0
    var incorrect: Int = 0
    var star: Int = 0
    
    var gameMode: Int = 0
    var sender: AnyObject?
    
    required init(score: Int, correct: Int, incorrect: Int, star: Int, level : Int, gameMode: Int, sender: AnyObject? = nil){
        self.score = score
        self.correct = correct
        self.incorrect = incorrect
        self.level = level
        self.star = star
        self.gameMode = gameMode
        self.sender = sender
    }
}